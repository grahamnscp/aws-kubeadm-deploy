apiVersion: kubeadm.k8s.io/v1beta2
kind: JoinConfiguration
discovery:
  bootstrapToken:
    token: {{ token }}
    apiServerEndpoint: {{ admin_external_fqdn }}:6443
    caCertHashes: ["{{ discovery_token_ca_cert_hash }}"]
nodeRegistration:
  name: ##THIS_HOSTNAME##
  kubeletExtraArgs:
    cloud-provider: aws
